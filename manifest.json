{
    "manifest_version": 2,
    "name": "文卓小宝贝儿",
    "version": "1.0",
    "description": "我的小宝贝儿又漂亮，又可爱，哈哈哈!",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },
    "permissions": [
        "activeTab",
        "declarativeContent",
        "cookies",
        "storage",
        "webNavigation",
        "tabs",
        "contextMenus",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "options_page": "options.html",
    "page_action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html"
    },
    "commands": {
        "_execute_browser_action": {
          "suggested_key": {
            "default": "Ctrl+Shift+F",
            "mac": "MacCtrl+Shift+F"
          },
          "description": "Opens hello.html"
        }
      }
}